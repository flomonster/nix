[pull]
	rebase = True
[core]
	editor = nvim
[gpg]
	program = gpg2
[commit]
	gpgsign = false
	verbose = true
[log]
	date = local
[advice]
	detachedHead = false
[rebase]
	autoSquash = true
[rerere]
	enabled = true
[alias]
	fixup = "!git log -n 50 --oneline --no-merges | sk -d=' ' --preview='git show --color {1}' | cut -c -7 | xargs -o git commit --fixup"
